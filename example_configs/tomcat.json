{
  "rules": [
    {
      "pattern": "Catalina<type=GlobalRequestProcessor, name=\"(\w+-\w+)-(\d+)\"><>requestCount:",
      "name": "tomcat_request_count",
      "labels": {
        "impl":"$1",
        "port":"$2" },
      "help": "Tomcat request count",
      "type": "COUNTER",
      "attrNameSnakeCase": false
    },
    {
      "pattern": "Catalina<type=GlobalRequestProcessor, name=\"(\w+-\w+)-(\d+)\"><>bytesSent:",
      "name": "tomcat_request_send_bytes",
      "labels": {
        "impl":"$1",
        "port":"$2" },
      "help": "Tomcat request send bytes",
      "type": "COUNTER",
      "attrNameSnakeCase": false
    },
    {
      "pattern": "Catalina<j2eeType=Servlet, WebModule=//([-a-zA-Z0-9+&@#/%?=~_|!:.,;]*[-a-zA-Z0-9+&@#/%=~_|]), name=([-a-zA-Z0-9+/$%~_-|!.]*), J2EEApplication=none, J2EEServer=none><>requestCount:",
      "name": "tomcat_request_servlet_count",
      "labels": {
        "module":"$1",
        "servlet":"$2" },
      "help": "Tomcat servlet request count",
      "type": "COUNTER",
      "attrNameSnakeCase": false
    },
  ]
}
